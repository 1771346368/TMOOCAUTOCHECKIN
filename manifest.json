{
  "manifest_version": 2,
  "name": "TMOOC自动签到",
  "version": "1.3.2",
  "description": "TMOOC自动签到",
  "icons": {
    "16": "img/logo.png",
    "48": "img/logo.png",
    "128": "img/logo.png"
  },
  "browser_action": {
    "default_icon": "img/logo.png",
    "default_title": "TMOOC自动签到",
    "default_popup": "src/popup.html"
  },
  "background": {
    "scripts": [
      "src/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/start.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "permissions": [
    "tabs",
    "storage",
    "notifications",
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "src/storage.js"
  ],
  "homepage_url": "https://drghost.top"
}